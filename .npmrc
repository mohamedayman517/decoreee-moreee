# Railway deployment optimizations
cache=/tmp/.npm
update-notifier=false
fund=false
audit=false
progress=false
loglevel=error

# Production settings
omit=dev
prefer-offline=true
no-optional=true

# Performance optimizations
maxsockets=1
fetch-retries=2
fetch-retry-factor=2
fetch-retry-mintimeout=10000
fetch-retry-maxtimeout=60000
